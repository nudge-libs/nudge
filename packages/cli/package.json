{
  "name": "@nudge/cli",
  "version": "0.0.1-beta.1",
  "description": "CLI for Nudge - generate type-safe prompt files for AI applications",
  "type": "module",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nudge-libs/nudge.git",
    "directory": "packages/cli"
  },
  "keywords": [
    "ai",
    "llm",
    "prompt",
    "prompt-engineering",
    "typescript",
    "cli",
    "codegen"
  ],
  "bin": {
    "nudge": "./dist/bin.cjs"
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "start": "ts-node src/index.ts",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@nudge/core": "workspace:*",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "tsdown": "^0.19.0",
    "typescript": "^5.7.0"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.cts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./bin": {
      "require": "./dist/bin.cjs",
      "import": "./dist/bin.mjs"
    },
    "./package.json": "./package.json"
  }
}
